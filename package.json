{
  "name": "keycloakify-starter",
  "homepage": "https://starter.keycloakify.dev",
  "version": "4.7.1",
  "description": "A starter/demo project for keycloakify",
  "repository": {
    "type": "git",
    "url": "git://github.com/codegouvfr/keycloakify-starter.git"
  },
  "scripts": {
    "postinstall": "copy-keycloak-resources-to-public",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-keycloak-theme": "yarn build && keycloakify",
    "storybook": "start-storybook -p 6006",
    "build-babel": "NODE_ENV=production BUILDER=babel webpack --config webpack/webpack.config.js && keycloakify",
    "build-ts": "NODE_ENV=production BUILDER=ts webpack --config webpack/webpack.config.js && keycloakify",
    "build-esbuild": "NODE_ENV=production BUILDER=esbuild webpack --config webpack/webpack.config.js && keycloakify",
    "build-babel-runtime": "NODE_ENV=production BUILDER=runtime webpack --config webpack/webpack.config.js && keycloakify",
    "build-minimal": "NODE_ENV=production BUILDER=minimal webpack --config webpack/webpack.config.js && keycloakify"
  },
  "keycloakify": {
    "extraThemeProperties": [
      "foo=bar"
    ],
    "extraThemeNames": [
      "keycloakify-starter-variant-1"
    ]
  },
  "author": "u/garronej",
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "evt": "^2.4.15",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^21.0.1",
    "keycloakify": "^8.1.0",
    "powerhooks": "^0.26.8",
    "react": "18.1.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "18.1.0",
    "tsafe": "^1.6.0"
  },
  "devDependencies": {
    "esbuild-loader": "^4.0.2",
    "esbuild-plugin-browserslist": "^0.9.0",
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^15.3.1",
    "@types/react": "18.0.9",
    "@types/react-dom": "^18.2.7",
    "babel-loader": "^9.1.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "browserslist": "^4.18.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "event-hooks-webpack-plugin": "^2.3.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.4",
    "resolve-url-loader": "^5.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.9.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "@typescript-eslint/no-redeclare": "off",
      "no-labels": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
